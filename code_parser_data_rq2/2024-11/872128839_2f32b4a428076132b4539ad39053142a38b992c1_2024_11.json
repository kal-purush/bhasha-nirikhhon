    "literals": [
        "\"Provider=Microsoft.ACE.OLEDB.12.0;Data Source=../../BD/DB.accdb\"",
        "\"SELECT IdCategoria, Categoria FROM CategoriasProveedores\"",
        "\"Categoria\"",
        "\"IdCategoria\"",
        "\"SELECT IdEstado, Nombre FROM Estado\"",
        "\"Nombre\"",
        "\"IdEstado\"",
        "\"SELECT p.IdProveedor, p.Nombre AS NombreProveedor, c.Categoria, e.Nombre AS EstadoProveedor, p.Direccion, p.Telefono, p.Correo, p.FechaInicio \"",
        "\"FROM (Proveedor p INNER JOIN Estado e ON p.IdEstado = e.IdEstado) INNER JOIN CategoriasProveedores c ON p.IdCategoria = c.IdCategoria \"",
        "\"ORDER BY p.IdProveedor\"",
        "\"SELECT IdProveedor, Nombre FROM Proveedor\"",
        "\"Nombre\"",
        "\"IdProveedor\"",
        "\"INSERT INTO Proveedor (Nombre, IdCategoria, IdEstado, Direccion, Telefono, Correo, FechaInicio) VALUES \"",
        "\"(?, ?, ?, ?, ?, ?, ?)\"",
        "\"@Nombre\"",
        "\"@IdCategoria\"",
        "\"@IdEstado\"",
        "\"@Direccion\"",
        "\"@Telefono\"",
        "\"@Correo\"",
        "\"@FechaInicio\"",
        "\"Se ha registrado el proveedor\"",
        "\"Atención\"",
        "\"IdProveedor\"",
        "\"UPDATE Proveedor SET Nombre = ?, IdCategoria = ?, IdEstado = ?, Direccion = ?, Telefono = ?, Correo = ?, FechaInicio = ? WHERE IdProveedor = ?\"",
        "\"@Nombre\"",
        "\"@IdCategoria\"",
        "\"@IdEstado\"",
        "\"@Direccion\"",
        "\"@Telefono\"",
        "\"@Correo\"",
        "\"@FechaInicio\"",
        "\"@IdProveedor\"",
        "\"Se han modificado los datos del proveedor\"",
        "\"Atención\"",
        "\"SELECT p.IdProducto, p.Nombre, p.Descripcion, p.Precio, c.Nombre, e.Nombre FROM ((Productos p INNER JOIN [Producto-Proveedor] pp ON p.IdProducto = pp.IdProducto) \"",
        "\"INNER JOIN Categorias c ON p.Categoria = c.IdCategoria) INNER JOIN Estado e ON p.Estado = e.IdEstado WHERE pp.IdProveedor = @IdProveedor\"",
        "\"@IdProveedor\"",
        "\"FROM (Proveedor p INNER JOIN CategoriasProveedores c ON p.IdCategoria = c.IdCategoria) WHERE p.Nombre = ?\"",
        "\"?\"",
        "\"Error al obtener los datos del proveedor: \""
