    "literals": [
        "\"Usage:  java coproc2 \"",
        "\"stylesheet xmlfile http://coproc2endpoint\"",
        "\"Content-Length\"",
        "\"Content-Type\"",
        "\"text/xml; charset=utf-8\"",
        "\"xsl\"",
        "\"POST\""
    ],
