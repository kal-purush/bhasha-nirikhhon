    "literals": [
        "\"react\"",
        "\"pure-react-carousel\"",
        "\"gatsby\"",
        "\"./carousel.module.scss\"",
        "query {\n      reviews: allContentfulReview(\n        sort: { fields: createdAt, order: DESC }\n        limit: 5\n      ) {\n        nodes {\n          author\n          content {\n            content\n            childMarkdownRemark {\n              html\n            }\n          }\n          link\n        }\n      }\n    }",
        "\"auto\"",
        "\"content\"",
        "\"right\"",
        "\"2em\"",
        "\" \"",
        "\"has-text-centered\"",
        "\"15px\"",
        "\"button is-rounded is-small is-success\"",
        "\"2px\""
    ],
    "variables": [
        "responsive"
    ],
    "comments": [
        "import Carousel from \"react-multi-carousel\"",
        "import \"react-multi-carousel/lib/styles.css\"",
        "optional, default to 1.",
        "optional, default to 1.",
        "optional, default to 1.",
        "return (",
        "<Carousel",
        "responsive={responsive}",
        "showDots={true}",
        "infinite={true}",
        "ssr={true}",
        "removeArrowOnDeviceType={[\"tablet\", \"mobile\", \"desktop\"]}",
        "autoPlay={true}",
        "autoPlaySpeed={5000}",
        "// customTransition=\"all .5\"",
        "itemClass={styles.item}",
        "centerMode={false}",
        "partialVisible={false}",
        "renderDotsOutside={true}",
        "dotListClass='dots'",
        ">",
        "{reviews.nodes.map(({ author, content, link }) => {",
        "return (",
        "<div class=\"box\">",
        "<p",
        "dangerouslySetInnerHTML={{",
        "__html: content.childMarkdownRemark.html,",
        "}}",
        "/>",
        "<p>",
        "<em>- {author}</em>",
        "</p>",
        "</div>",
        ")",
        "})}",
        "</Carousel>",
        ")"
    ],
