    "docstrings": [
        "\"\"\"\nSpot Info  config  file parser.\n\"\"\"",
        "\"\"\"To parse spot report info config file\n    \"\"\"",
        "\"\"\"Set the file path, and which file to parser.\"\"\"",
        "\"\"\"Read self._file_path and return the lines needed.\n\n        Need to do filter operations:\n            1. lines full with spaces\n            2. comments\n            3. comments starts with spaces.\n        \"\"\"",
        "\"\"\"Check name exists in this file.\"\"\"",
        "\"\"\"Transfer the manifest file into json format dict\"\"\"",
        "\"\"\"ssh %s \" %s >/tmp/%s \" \"\"\"",
        "\"\"\"scp  %s:/tmp/%s %s/ \"\"\"",
        "\"\"\"ssh %s \"ceph osd getcrushmap -o /tmp/%s \" \"\"\"",
        "\"\"\"ssh %s \" %s >/tmp/%s \" \"\"\"",
        "\"\"\"scp  %s:/tmp/%s %s/ \"\"\"",
        "\"\"\"scp  %s:%s %s/ \"\"\"",
        "\"\"\"mysqldump -uroot -p`cat /etc/vsmdeploy/deployrc | grep ROOT | awk -F \"=\" '{print $2}'` --opt --events --all-databases > %s/%s\"\"\"",
        "\"\"\"%s -d %s/ \"\"\"",
        "\"\"\"ssh %s \" %s >/tmp/%s \" \"\"\"",
        "\"\"\"scp  %s:/tmp/%s %s/%s/os/ \"\"\"",
        "\"\"\" %s >%s/os/%s \"\"\"",
        "\"\"\"scp  %s:%s %s/%s/os/ \"\"\"",
        "\"\"\"cp  %s %s/os/ \"\"\""
    ],
