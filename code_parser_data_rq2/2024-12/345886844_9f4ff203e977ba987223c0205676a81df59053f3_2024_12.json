    "literals": [
        "'auger_oca'",
        "\"auger_oca package is not installed. USE: pip install auger-oca \"",
        "'auger_outputs'",
        "'rm -r auger_outputs'",
        "'mkdir auger_outputs'",
        "'r2TM_*'",
        "r'/$'",
        "''",
        "\"/\"",
        "'mv Auger_OCA.* auger_outputs'",
        "'for f in $(ls auger_outputs/Auger_OCA.*); do grep \"Spectrum: BE(eV) and Intensity\" $f \\\n >> tmp.spectrum.out; done ; cat tmp.spectrum.out | cut -b 1-42 --complement >> auger.spectrum.out; rm -f tmp.spectrum.out'",
