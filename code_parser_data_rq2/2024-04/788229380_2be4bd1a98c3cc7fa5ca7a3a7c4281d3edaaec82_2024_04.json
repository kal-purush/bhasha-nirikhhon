    "literals": [
        "\"background_color\"",
        "\"#E3BC9A\"",
        "\"Advanced \\\\LaTeX\\\\\"",
        "\"made by Sherstobitov Andrei\"",
        "\"powered by Manim Community & Manim Slides\"",
        "\"Today we'll discuss:\"",
        "\"1. How to tex on your computer.\"",
        "\"2. How to use cross references.\"",
        "\"3. How to add graphics using tex.\"",
        "\"Let's consider Overleaf\"",
        "\"assets/images/overleaf_before_compile.png\"",
        "\"Waiting...\"",
        "\"assets/images/overleaf_timeout_error.png\"",
        "\"assets/images/overleaf_subscribe.png\"",
        "\"console\"",
        "\"native\"",
        "\"rectange\"",
        "\"Install latexmk.\"",
        "\"On Linux:\"",
        "\"perl --version\"",
        "\"1. Perl (www.perl.org) should be already installed.\"",
        "\"sudo apt install -y latexmk\"",
        "\"2. Install a package called latexmk, i.e. on Ubuntu:\"",
        "\"On macOS:\"",
        "\"brew install --cask mactex\"",
        "\"sudo tlmgr install latexmk\"",
        "\"On Windows:\"",
        "\"1. Install MikTex: https://miktex.org/howto/install-miktex\"",
        "\"2. Install Perl for Windows: https://strawberryperl.com\"",
        "\"3. Use the MikTeX Package Manager to install latexmk.\"",
        "\"latex\"",
        "\"vim\"",
        "\"window\"",
        "\"Reference sections and chapters by hand.\"",
        "\"assets/tex/cross-ref-sections-by-hand-before-change.tex\"",
        "\"assets/images/cross-ref-sections-by-hand-before-change.png\"",
        "\"assets/tex/cross-ref-sections-by-hand-after-change.tex\"",
        "\"assets/images/cross-ref-sections-by-hand-after-change.png\"",
        "\"Reference sections and chapters automatically.\"",
        "\"assets/tex/cross-ref-sections.tex\"",
        "\"latexmk -pdf assets/tex/cross-ref-sections.tex\"",
        "\"assets/images/cross-ref-sections.png\"",
        "\"Reference images.\"",
        "\"assets/tex/cross-ref-image.tex\"",
        "\"latexmk -pdf assets/tex/cross-ref-image.tex\"",
        "\"assets/images/cross-ref-image.png\"",
        "\"Reference equations.\"",
        "\"assets/tex/cross-ref-eqs.tex\"",
        "\"latexmk -pdf assets/tex/cross-ref-eqs.tex\"",
        "\"assets/images/cross-ref-eqs.png\"",
        "\"Introduction to TikZ\"",
        "\"What is TikZ?\"",
        "\"Why use TikZ?\"",
        "\"sudo apt install -y texlive-latex-base\"",
        "\"Install TikZ and necessities on Ubuntu:\"",
        "\"On macOS and on Windows through their packet managers.\"",
        "\"Euclid's elements. Book I, Proposition I.\"",
        "\"assets/tex/tikz-euclid-line.tex\"",
        "\"assets/images/tikz-euclid-line.png\"",
        "\"assets/tex/tikz-euclid-one-circle.tex\"",
        "\"assets/images/tikz-euclid-one-circle.png\"",
        "\"assets/tex/tikz-euclid-two-circles.tex\"",
        "\"assets/images/tikz-euclid-two-circles.png\"",
        "\"assets/tex/tikz-euclid-intersection.tex\"",
        "\"assets/images/tikz-euclid-intersection.png\"",
        "\"And much more...\"",
        "\"assets/tex/tikz-euclid.tex\"",
        "\"assets/images/tikz-euclid.png\"",
        "\"Plotting graphics using TikZ.\"",
        "\"Install gnuplot\"",
        "\"Windows using Chocolatey\"",
        "\"choco install gnuplot\"",
        "\"Ubuntu\"",
        "\"sudo apt install gnuplot\"",
        "\"MacOS\"",
        "\"brew install gnuplot\"",
        "\"Chebyshev Polynomials\"",
        "\"T_n\"",
        "\"(\"",
        "\"\\\\cos\"",
        "\"\\\\theta\"",
        "\")\"",
        "\"=\"",
        "\"\\\\cos\"",
        "\"(\"",
        "\"n\"",
        "\"\\\\theta\"",
        "\")\"",
        "\"T_n\"",
        "\"(\"",
        "\"x\"",
        "\")\"",
        "\"=\"",
        "\"\\\\cos\"",
        "\"(\"",
        "\"n\"",
        "\"\\\\arccos\"",
        "\"x\"",
        "\")\"",
        "\",\\\\ |x|\\\\leq 1\"",
        "\"assets/tex/chebyshev-trigo.tex\"",
        "\"assets/images/chebyshev-trigo.png\"",
        "\"T_{n+1}(x)=2xT_n(x)-T_{n-1}(x),\\\\ T_0(x)=1,\\\\ T_1(x)=x\"",
        "\"T_n(x)=\\\\frac{(x+\\\\sqrt{x^2 - 1})^n + (x-\\\\sqrt{x^2-1})^n}{2},\\\\ x\\\\in\\\\mathbf{R}\"",
        "\"assets/tex/chebyshev-poly.tex\"",
        "\"assets/images/chebyshev-poly.png\"",
        "\"Three Dimensional Drawing.\"",
        "\"assets/tex/tikz-3d-graphic.tex\"",
        "\"assets/images/tikz-3d-graphic.png\"",
        "\"Thank you for your attention!\""
    ],
