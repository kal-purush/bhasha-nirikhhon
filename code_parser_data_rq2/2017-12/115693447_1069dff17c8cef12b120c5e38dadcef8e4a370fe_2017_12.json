        "\"Aurelien FORET\"",
        "\"0.4\"",
        "'pactest-output-'",
        "\"usage: %prog [options] <path/to/testfile.py>...\"",
        "\"Runs automated tests on the pacman binary. Tests are \"",
        "\"described using an easy python syntax, and several can be \"",
        "\"ran at once.\"",
        "\"-v\"",
        "\"--verbose\"",
        "\"count\"",
        "\"verbose\"",
        "\"print verbose output\"",
        "\"-d\"",
        "\"--debug\"",
        "\"int\"",
        "\"debug\"",
        "\"set debug level for pacman\"",
        "\"-p\"",
        "\"--pacman\"",
        "\"string\"",
        "\"bin\"",
        "\"specify location of the pacman binary\"",
        "\"--bindir\"",
        "\"string\"",
        "\"bindir\"",
        "\"append\"",
        "\"specify location of binaries\"",
        "\"--keep-root\"",
        "\"store_true\"",
        "\"keeproot\"",
        "\"don't remove the generated pacman root filesystem\"",
        "\"--nolog\"",
        "\"store_true\"",
        "\"nolog\"",
        "\"do not log pacman messages\"",
        "\"--gdb\"",
        "\"store_true\"",
        "\"gdb\"",
        "\"use gdb while calling pacman\"",
        "\"--valgrind\"",
        "\"store_true\"",
        "\"valgrind\"",
        "\"use valgrind while calling pacman\"",
        "\"--manual-confirm\"",
        "\"store_true\"",
        "\"manualconfirm\"",
        "\"do not use --noconfirm for pacman calls\"",
        "\"--scriptlet-shell\"",
        "\"string\"",
        "\"scriptletshell\"",
        "\"/bin/sh\"",
        "\"specify path to shell used for install scriptlets\"",
        "\"--ldconfig\"",
        "\"string\"",
        "\"ldconfig\"",
        "\"/sbin/ldconfig\"",
        "\"specify path to ldconfig\"",
        "\"--review\"",
        "\"store_true\"",
        "\"review\"",
        "\"review test files, test output, and saved logs\"",
        "\"--editor\"",
        "\"store\"",
        "\"editor\"",
        "'EDITOR'",
        "'vim'",
        "\"editor to use for viewing files\"",
        "\"__main__\"",
        "\"Python versions before 2.7 are not supported.\"",
        "\"no tests defined, nothing to do\"",
        "'pactest-'",
