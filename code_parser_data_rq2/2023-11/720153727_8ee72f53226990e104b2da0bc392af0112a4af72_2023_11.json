        "packet.Add(\"m\", \"AirdropLootPacket\");",
        "packet.Add(\"config\", config);",
        "packet.Add(\"result\", lootData);",
        "AkiBackendCommunication.Instance.SendDataToPool(packet.SITToJson());",
