        "f\"\"\"\n            QListWidget {{\n                background-color: {list_bg};\n                border: 1px solid {border_color};\n                border-radius: 4px;\n                color: {text_color};\n            }}\n            QListWidget::item {{\n                padding: 5px;\n                border-radius: 2px;\n            }}\n            QListWidget::item:selected {{\n                background-color: {accent_color};\n                color: white;\n            }}\n            QListWidget::item:hover {{\n                background-color: {\"#4a4a4a\" if is_dark else \"#f0f0f0\"};\n            }}\n        \"\"\"",
        "\"#4a4a4a\"",
