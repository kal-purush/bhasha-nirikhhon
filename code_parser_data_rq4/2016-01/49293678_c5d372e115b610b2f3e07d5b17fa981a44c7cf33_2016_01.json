    "literals": [
        "\"IgPdf\"",
        "\"-h\"",
        "\"--help\"",
        "\".pdf\"",
        "\"The 'x_location' must be a non-negative integer, not '\"",
        "\"'.\"",
        "\"The 'y_location' must be a non-negative integer, not '\"",
        "\"'.\"",
        "\"The 'resulting_width' must be a non-negative integer, not '\"",
        "\"'.\"",
        "\"The 'resulting_height' must be a non-negative integer, not '\"",
        "\"'.\"",
        "\"The 'compression_type' must be a non-negative integer, not '\"",
        "\"'.\"",
        "\"The 'use_buffered_image' must be a boolean, not '\"",
        "\"'.\"",
        "\"Usage:\"",
        "\"       AddImageSample.jar <input_file_path> [<output_file_path> [<x_location> <y_location> <resulting_width> <resulting_height> [<compression_type> [<use_buffered_image>]]]]\"",
        "\"where:\"",
        "\"       <input_file_path>:       The path to source image file.\"",
        "\"       <output_file_path>:      The path to output PDF file (default is <input_file_path>.pdf).\"",
        "\"       <x_location>:            The Horizontal location of image (default is 0).\"",
        "\"       <y_location>:            The Vertical location of image (default is 0).\"",
        "\"       <resulting_width>:       The resulting image width (default is 0 - original width).\"",
        "\"       <resulting_height>:      The resulting image height (default is 0 - original height).\"",
        "\"       <compression_type>:      The resulting image compression type (default is 0 - no compression).\"",
        "\"       <use_buffered_image>:    The indicator that image file should be processed as BufferedImage (default is 'false').\"",
        "\"IOException: \"",
        "\"Exception: \"",
        "\"png\"",
        "\"The source image could not be written as a PNG image stream.\""
    ],
