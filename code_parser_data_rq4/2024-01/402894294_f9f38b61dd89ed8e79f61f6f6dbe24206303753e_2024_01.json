    "comments": [
        "Get the ctsm util tools and then the cime tools.",
        ", \"sasu\"],",
        "parser.add_argument(",
        "\"--start-date\",",
        "help=\"\"\"",
        "Start date for running CTSM simulation in ISO format.",
        "[default: %(default)s]",
        "(currently non-functional)",
        "\"\"\",",
        "action=\"store\",",
        "dest=\"start_date\",",
        "required=False,",
        "type=datetime.date.fromisoformat,",
        "default=datetime.datetime.strptime(\"2018-01-01\", \"%Y-%m-%d\"),",
        ")",
        "parser.add_argument(",
        "\"--end-date\",",
        "help=\"\"\"",
        "End date for running CTSM simulation in ISO format.",
        "[default: %(default)s]",
        "\"\"\",",
        "action=\"store\",",
        "dest=\"end_date\",",
        "required=False,",
        "type=datetime.date.fromisoformat,",
        "default=datetime.datetime.strptime(\"2021-01-01\", \"%Y-%m-%d\"),",
        ")",
        "The transient run length is set by cdeps atm buildnml to",
        "the last date of the available tower data",
        "this value is not used",
        "Reduce output level for this script unless --debug or",
        "--verbose is provided on the command line"
    ],
    "docstrings": [
        "\"\"\"\nArgument parser to use throughout run_neon.py\n\"\"\"",
        "\"\"\"\n    Get parser object for this script.\n    \"\"\"",
        "\"\"\"\n                Root Directory of base case build\n                [default: %(default)s]\n                \"\"\"",
        "\"\"\"\n                Root output directory of cases\n                [default: %(default)s]\n                \"\"\"",
        "\"\"\"\n                overwrite existing case directories\n                [default: %(default)s]\n                \"\"\"",
        "\"\"\"\n                Only setup the requested cases, do not build or run\n                [default: %(default)s]\n                \"\"\"",
        "\"\"\"\n                If the case exists but does not appear to be complete, restart it.\n                [default: %(default)s]\n                \"\"\"",
        "\"\"\"\n                Run locally, do not use batch queueing system (if defined for Machine)\n                [default: %(default)s]\n                \"\"\"",
        "\"\"\"\n                        Type of run to do\n                        [default: %(default)s]\n                        \"\"\"",
        "\"\"\"\n                Uses the PRISM reanaylsis precipitation data for the site instead of the NEON data\n                (only available over Continental US)\n                \"\"\"",
        "\"\"\"\n                Appends the case name with string for model experiment\n                \"\"\"",
        "\"\"\"\n                How long to run (modified ISO 8601 duration)\n                [default: %(default)s]\n                \"\"\"",
        "\"\"\"\n                        For transient runs only - should we start from the postad spinup or finidat?\n                        By default start from finidat, if this flag is used the postad run must be available.\n                        \"\"\"",
        "\"\"\"\n                Neon data version to use for this simulation.\n                [default: use the latest data available]\n                \"\"\""
    ],
