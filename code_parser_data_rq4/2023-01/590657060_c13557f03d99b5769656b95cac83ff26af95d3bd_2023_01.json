        "db.session.add(new_book)",
        "db.session.commit()",
        "------------ READ TABLE",
        "all_books = Book.query.all()",
        "print(all_books)",
        "------------ EDIT ENTRY BY QUERY(SEARCH)",
        "book_to_update = Book.query.filter_by(title=\"Harry Potter\").first()",
