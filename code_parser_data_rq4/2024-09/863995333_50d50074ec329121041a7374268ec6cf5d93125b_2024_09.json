        "<!doctype html>\n<html lang=\"sk\">\n  <head>\n    <meta charset=\"utf-8\" />\n    <meta content=\"viewport-fit=cover, width=device-width\" name=\"viewport\" />\n\n    <style>\n      a {\n        color: inherit;\n        text-decoration-line: none;\n      }\n      body {\n        background-color: hsl(0, 0%, 12.5%);\n        color: hsl(0, 0%, 87.5%);\n        font-family: ui-monospace;\n        line-height: 1.25;\n        margin: 32px;\n      }\n      h1 {\n        font-size: 32px;\n        font-weight: 300;\n        margin: 32px 0;\n      }\n      p {\n        margin: 16px 0;\n      }\n      .messages {\n        display: grid;\n        gap: 16px;\n        grid-template-columns: repeat(3, 1fr);\n      }\n      @media (max-width: 1024px) {\n        .messages {\n          grid-template-columns: 1fr;\n        }\n      }\n      .message {\n        align-items: center;\n        background-color: hsl(0, 0%, 25%);\n        border: 2px solid hsl(0, 0%, 37.5%);\n        border-radius: 2px;\n        column-gap: 16px;\n        display: flex;\n        padding: 16px;\n      }\n      .message__date,\n      .message__left,\n      .message__repository {\n        opacity: 0.5;\n      }\n    </style>\n\n    <title>Prehľad (${filteredMessages.length})</title>\n  </head>\n  <body>\n    <h1>Prehľad (${filteredMessages.length})</h1>\n\n    <p>${repositories.map(({ name }, i) => `${i + 1}. ${name}`).join(', ')}</p>\n    <p>Od ${filter.toLocaleDateString('sk')}</p>\n\n    <div class=\"messages\">",
        "${i + 1}. ${name}",
        "', '",
        "'sk'",
        "''",
        "<a class=\"message\" href=\"${repositoryUrl}/commit/${hash}\" target=\"_blank\">",
        "<div class=\"message__left\">${(i + 1).toString().padStart(3, '0')}</div>",
        "'0'",
        "'        <div class=\"message__right\">'",
        "<div class=\"message__repository\">${repository.name}</div>",
        "<div class=\"message__date\">${date.toLocaleString('sk')}</div>",
        "'sk'",
        "<div class=\"message__message\">${message.replace(/^commit:\\s/, '')}</div>",
        "''",
        "'        </div>'",
        "'      </a>'",
        "</div>\n\n    <p>${new Date().toLocaleString('sk')}</p>\n    <a href=\"https://www.instagram.com/marekkobida\" target=\"_blank\">Copyright 2024 Marek Kobida</a>\n  </body>\n</html>",
        "'sk'",
