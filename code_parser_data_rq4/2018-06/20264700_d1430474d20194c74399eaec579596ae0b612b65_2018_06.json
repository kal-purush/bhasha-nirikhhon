    "docstrings": [
        "\"\"\"<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n<testsuite\n  name=\"%(testname)s\"\n  tests=\"%(test_count)d\"\n  failures=\"%(error_count)d\"\n  time=\"%(time)s\"\n>\n\"\"\"",
        "\"\"\"  <testcase\n    name=%(quoted_name)s\n    classname=\"%(testname)s\"\n  >\n      <failure message=%(quoted_message)s/>\n  </testcase>\n\"\"\"",
        "\"\"\"  <testcase\n    name=%(quoted_location)s\n    classname=\"%(testname)s\"\n    status=\"No errors\"/>\n\"\"\"",
        "\"\"\"  <system-out>Checked files:%(escaped_files)s</system-out>\n\"\"\""
