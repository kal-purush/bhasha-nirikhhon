    "literals": [
        "\"nestedpages\"",
        "\"count\"",
        "\"document\"",
        "\"local\"",
        "\"WebHome\"",
        "\", XWikiSpace AS space WHERE doc.space = space.reference\"",
        "\" AND doc.name = 'WebHome' AND space.parent IS NULL\"",
        "\" AND ((doc.name <> 'WebHome' AND doc.space = :parent) OR\"",
        "\" (doc.name = 'WebHome' AND space.parent = :parent))\"",
        "\" AND space.name LIKE :text\"",
        "\" AND ((doc.name <> 'WebHome' AND doc.name like :text) OR\"",
        "\" (doc.name = 'WebHome' AND space.name LIKE :text))\"",
        "\" ORDER BY doc.fullName\"",
        "\"parent\"",
        "\"text\""
