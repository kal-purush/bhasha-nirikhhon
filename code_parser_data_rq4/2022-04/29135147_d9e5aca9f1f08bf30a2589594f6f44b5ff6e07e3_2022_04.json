    "docstrings": [
        "\"\"\"==============  Insteon PowerLinc modem (PLM) ===============\"\"\"",
        "\"\"\"getdb()\n\t\tdownload the modem database and print it on the console\"\"\"",
        "\"\"\"readdb()\n\t\tdownload the modem database and return it\"\"\"",
        "\"\"\"startWatch()\n\t\tmodem will print all incoming messages on terminal\"\"\"",
        "\"\"\"stopWatch()\n\t\tstop modem from printing all incoming messages on terminal\"\"\"",
        "\"\"\"getid()\n\t\tget modem id data\"\"\"",
        "\"\"\"getIMConfig()\n\t\tget modem configuration flags byte\"\"\"",
        "\"\"\"sendOn(group)\n\t\tsends ALLLink broadcast ON message to group \"group\" \"\"\"",
        "\"\"\"sendOff(group)\n\t\tsends ALLLink broadcast OFF message to group \"group\" \"\"\"",
        "\"\"\"linkAsController(group)\n\t\tputs modem in link mode on group \"group\" \"\"\"",
        "\"\"\"linkAsResponder(group)\n\t\tputs modem in link mode on group \"group\" \"\"\"",
        "\"\"\"linkAsEither(group)\n\t\tputs modem in link mode to link as controller or responder on group \"group\" \"\"\"",
        "\"\"\"respondToUnlink(group)\n\t\tmake modem respond to unlink message\"\"\"",
        "\"\"\"unlinkAsController(group)\n\t\tputs modem in unlink mode to unlink as controller on group \"group\" \"\"\"",
        "\"\"\"cancelLinking()\n\t\ttakes modem out of linking or unlinking mode\"\"\"",
        "\"\"\"addController(addr, group):\n\t\tadds device with address \"addr\" to modem link database as controller for group \"group\" \"\"\"",
        "\"\"\"addResponder(addr, group[, data]):\n\t\tadds device with address \"addr\" to modem link database as responder to group \"group\" \"\"\"",
        "\"\"\"addSoftwareResponder(addr):\n\t\tadds device with address \"addr\" to modem link database as software responder\"\"\"",
        "\"\"\"removeResponderOrController(addr, group)\n\t\tremoves device with address \"addr\" and group \"group\" from modem link database\"\"\"",
        "\"\"\"removeResponder(addr, group)\n\t\tcould not be implemented for the modem. Use removeResponderOrController() instead!\"\"\"",
        "\"\"\"removeController(addr, group)\n\t\tcould not be implemented for the modem. Use removeResponderOrController() instead!\"\"\"",
        "\"\"\"removeDevice(addr):\n\t\tremoves all links to device with address \"addr\" from modem database\"\"\"",
        "\"\"\"saveDB(filename)\n\t\tsave modem database to file \"filename\" \"\"\"",
        "\"\"\"loadDB(filename)\n\t\tload modem database from file \"filename\" (note: this will not change the actual modem db) \"\"\"",
        "\"\"\"nukeDB()\n\t\tdelete complete modem database! \"\"\"",
        "\"\"\"restoreDB(filename)\n\t\trestore modem database from file \"filename\" \"\"\""
    ],
